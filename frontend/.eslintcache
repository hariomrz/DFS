n                                                                value={selectedTeam}\n                                                                placeholder={AppLabels.SELECT_TEAM}\n                                                                isSearchable={false}\n                                                                isClearable={false}\n                                                                theme={(theme) => ({\n                                                                    ...theme,\n                                                                    borderRadius: 0,\n                                                                    colors: {\n                                                                        ...theme.colors,\n                                                                        primary25: '#fff',\n                                                                        primary: '#555555',\n                                                                    },\n                                                                })}\n                                                            />\n                                                            :\n                                                            <Select\n                                                                onChange={this.handleTeamChange}\n                                                                options={lineUpMasterIdOfCreatedTeam != \"\" ? TeamsSortedArray : TeamsSortedArray}\n                                                                className={\"basic-select-field\" + (lineUpMasterIdOfCreatedTeam != \"\" ? '' : '')}\n                                                                classNamePrefix=\"select\"\n                                                                value={selectedTeam}\n                                                                placeholder={AppLabels.SELECT_TEAM}\n                       